{
  "name": "vasquez-inmobiliaria",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:services:up": "docker-compose --env-file .env.development up -d",
    "dev:services:down": "docker-compose --env-file .env.development down",
    "dev": "env-cmd -f .env.development next dev",
    "build:dev": "env-cmd -f .env.development next build",
    "qa:services:up": "docker-compose --env-file .env.qa up -d",
    "qa:services:down": "docker-compose --env-file .env.qa down",
    "qa": "env-cmd -f .env.qa next dev",
    "build:qa": "env-cmd -f .env.qa next build",
    "prod": "env-cmd -f .env.production next dev",
    "build:prod": "env-cmd -f .env.production next build",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest --collect-coverage --no-cache --runInBand",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-label": "^2.1.7",
    "@types/nodemailer": "^7.0.1",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jose": "^6.1.0",
    "mongoose": "^8.18.1",
    "next": "15.5.3",
    "nodemailer": "^7.0.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "env-cmd": "^11.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-validate-filename": "^1.2.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
